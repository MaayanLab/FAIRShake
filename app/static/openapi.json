                      ]
                    },
                    "q5u": {
                      "description": "URL comment for question 5. Must be a valid URL",
                      "type": "string"
                    },
                    "q5c": {
                      "description": "Text comment for question 5",
                      "type": "string"
                    },
                    "q6a": {
                      "description": "Answer to question 6",
                      "type": "string",
                      "enum": [
